<?xml version="1.0" encoding="UTF-8"?>
<model xmlns="http://www.cellml.org/cellml/2.0#" name="multiple_math_in_component">
  <component name="environment">
    <variable name="nK_stim" units="dimensionless"/>
    <variable name="stimPeriod" units="second"/>
    <math xmlns="http://www.w3.org/1998/Math/MathML" xmlns:cellml="http://www.cellml.org/cellml/2.0#">
      <apply>
        <eq/>
        <ci>nK_stim</ci>
        <cn cellml:units="dimensionless">0.3</cn>
      </apply>
    </math>
    <math xmlns="http://www.w3.org/1998/Math/MathML" xmlns:cellml="http://www.cellml.org/cellml/2.0#">
      <apply>
        <eq/>
        <ci>stimPeriod</ci>
        <cn cellml:units="second">42.6</cn>
      </apply>
    </math>
  </component>
</model>
